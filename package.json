{
  "name": "@wecancer/design-system",
  "version": "1.0.29",
  "private": false,
  "homepage": "wecancer-design-system.netlify.app",
  "license": "MIT",
  "main": "dist/wecancer-design-system.cjs.js",
  "module": "dist/wecancer-design-system.esm.js",
  "engines": {
    "node": "^16.14.0"
  },
  "scripts": {
    "prepublishOnly": "yarn run check && yarn build",
    "test": "react-scripts test",
    "check": "CI=true yarn test && yarn run check:lint && yarn run check:prettier && yarn run check:types",
    "check:prettier": "prettier --check './src'",
    "check:lint": "eslint ./src",
    "check:types": "tsc --project ./tsconfig.json",
    "eject": "react-scripts eject",
    "start": "start-storybook -p 6006 -s public",
    "build": "preconstruct build",
    "build:demo": "build-storybook -s public -o demo",
    "build:babel": "BABEL_ENV=esm babel --source-maps --extensions \".js,.ts,.tsx\" src --out-dir dist/js --plugins \"inline-react-svg,@babel/plugin-proposal-optional-chaining\""
  },
  "dependencies": {
    "date-fns": "^2.28.0",
    "nanoid": "^3.1.30",
    "prop-types": "^15.8.1",
    "react-day-picker": "^8.0.0",
    "react-select": "^5.2.1",
    "react-select-async-paginate": "^0.6.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@preconstruct/cli": "^2.1.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/node-logger": "^6.4.9",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.9",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.18",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-plugin-typescript-to-proptypes": "^2.0.0",
    "concurrently": "^7.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-styled-components-a11y": "^0.0.34",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "5.0.0",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0",
    "webpack": "^5.65.0"
  },
  "peerDependencies": {
    "date-fns": "^2.28.0",
    "nanoid": "^3.1.30",
    "prop-types": "^15.8.1",
    "react": "^16.8.0 || ^17.0.0",
    "react-day-picker": "^8.0.0-beta.38",
    "react-dom": "^17.0.2",
    "react-select": "^5.2.1",
    "styled-components": "^5.3.3"
  },
  "publishConfig": {
    "access": "public",
    "tag": "prerelease"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "\\.svg": "<rootDir>/src/__mocks__/svg.tsx"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wecancer/design-system.git"
  },
  "packageManager": "yarn@3.2.0"
}
